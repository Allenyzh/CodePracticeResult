func turnaround(){
   Turnleft() 
   Turnleft()
}
func solvestair(){                                           
     moveForward()                           
     collectGem()                           
     turnAround()
     moveForward()                           
     moveForward()                           
     collectGem()                                      
     turnAround()
     moveForward()
     turnRight()
     moveForward()
     collectGem()
     turnAround()
     moveForward()
     moveForward()
     collectGem()
 }
  solvestair()
    
