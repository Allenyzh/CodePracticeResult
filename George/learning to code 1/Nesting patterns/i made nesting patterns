func turnaround(){
   Turnleft() 
   Turnleft()
}
func Solvestair(){                                           
     moveForward()                           
     collectGem()                           
     turnaround()
     moveForward()                           
     moveForward()                           
     collectGem()                                      
     turnAround()
     moveForward()
     turnRight()
     moveForward()
     collectGem()
     turnAround()
     moveForward()
     moveForward()
     collectGem()
 }
  Solvestair()
    
