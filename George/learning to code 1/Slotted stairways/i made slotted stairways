func  collectgemturnaround{
   Moveforeward()
   Moveforeward()
   Collectgem()
   Turnleft()
   Moveforeward()
 }
func SolveRow(){
    collectGemTurnAround()
    collectGemTurnAround()
    turnRight()
    moveForward()
    turnRight()
    collectGemTurnAround()
    turnRight()
    moveForward()
    turnLeft()
    collectGemTurnAround()
    collectGemTurnAround()
    turnLeft()
    moveForward()
    turnRight()
    collectGemTurnAround()
} 
solveRow()
    
    
 
 
  
   
