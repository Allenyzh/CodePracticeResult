func  collectgemturnaround{
   Moveforeward()
   Moveforeward()
   Collectgem()
   Turnleft()
   Moveforeward()
 }
func SolveRow(){
    solveRow()
    turnLeft()
    moveForward()
    turnRight()
    solveRow()
    turnRight()
    moveForward()
    turnLeft()
    solveRow()
    solveRow()
    turnLeft()
    moveForward()
    moveForward()
    turnLeft()
    solveRow()
  } 

    
    
 
 
  
   
