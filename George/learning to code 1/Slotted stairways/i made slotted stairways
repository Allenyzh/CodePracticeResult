func  collectGemTurnAround{
   moveForeward()
   moveForeward()
   collectGem()
   turnLeft()
   moveForeward()
 }
func solveRow(){
    collectGemTurnAround()
    collectGemTurnAround()
    turnRight()
    moveForward()
    turnRight()
    collectGemTurnAround()
    turnRight()
    moveForward()
    turnLeft()
    collectGemTurnAround()
    collectGemTurnAround()
    turnLeft()
    moveForward()
    turnRight()
    collectGemTurnAround()
} 
solveRow()
    
    
 
 
  
   
