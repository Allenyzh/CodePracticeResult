func  collectgemturnaround{
   moveforeward()
   moveforeward()
   collectgem()
   turnleft()
   moveforeward()
 }
func solveRow(){
    collectGemTurnAround()
    collectGemTurnAround()
    turnRight()
    moveForward()
    turnRight()
    collectGemTurnAround()
    turnRight()
    moveForward()
    turnLeft()
    collectGemTurnAround()
    collectGemTurnAround()
    turnLeft()
    moveForward()
    turnRight()
    collectGemTurnAround()
} 
solveRow()
    
    
 
 
  
   
